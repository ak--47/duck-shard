{
  "name": "duck-shard",
  "version": "1.1.01",
  "description": "Universal Data Pipeline - DuckDB ETL tool with web UI and jq transformations",
  "type": "module",
  "main": "server.mjs",
  "bin": {
    "duck-shard": "./bin/duck-shard.mjs"
  },
  "scripts": {
    "start": "node bin/duck-shard.mjs --ui",
    "dev": "node --watch server.mjs",
    "cli": "node bin/duck-shard.mjs",
    "test": "bats tests/test.bats",
    "deploy": "./deploy.sh",
    "health": "curl -f http://localhost:${PORT:-8080}/health || exit 1"
